<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dhu.cst.zjm.entity.Md5Entity" table="MD5" schema="encrypt">
        <id name="id">
            <column name="ID" sql-type="int(11)" unique="true"/>
        </id>
        <property name="sign">
            <column name="Sign" sql-type="mediumtext"/>
        </property>
        <many-to-one name="encryptRelationByRelationId" class="com.dhu.cst.zjm.entity.EncryptRelationEntity">
            <column name="RelationID" sql-type="int(11)" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>