<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dhu.cst.zjm.entity.base.EncryptTypeBaseEntity" table="EncryptType" schema="encrypt">
        <id name="id">
            <column name="ID" sql-type="int(11)" unique="true"/>
        </id>
        <property name="name">
            <column name="Name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="description">
            <column name="Description" sql-type="mediumtext"/>
        </property>
    </class>
</hibernate-mapping>