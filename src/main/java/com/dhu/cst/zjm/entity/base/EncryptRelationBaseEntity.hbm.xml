<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dhu.cst.zjm.entity.base.EncryptRelationBaseEntity" table="EncryptRelation" schema="encrypt">
        <id name="id">
            <column name="ID" sql-type="int(11)" unique="true"/>
        </id>
        <property name="fileId">
            <column name="FileID" sql-type="int(11)"/>
        </property>
        <property name="typeId">
            <column name="TypeID" sql-type="int(11)"/>
        </property>
        <property name="encryptTime">
            <column name="EncryptTime" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>